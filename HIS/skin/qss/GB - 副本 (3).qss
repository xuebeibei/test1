QMainWindow{
    background-color: rgb(240,240,240);
    background-image: url(none);
    background-repeat:repeat-xy;
}

QWigdet{
    background-color: transparent;
}
QLineEdit {
    padding: 1px;
    border-style: solid;
    border: 1px solid rgb(85,170,255);
    border-radius: 1px;
}
QDateEdit {
    padding: 1px;
    border-style: solid;
    border: 1px solid rgb(85,170,255);
    border-radius: 5px;
    width:40px;
    height: 30px;
	border-width: 1px;
}
QToolBar {
    background: rgb(85,170,255);
    spacing: 0px; /* spacing between items in the tool bar */
}


QProgressBar {
    border: 1px solid white;
    text-align: right;
}

QProgressBar::chunk {
    background-color: lightgreen;
    width: 20px;
}

QProgressBar QLineEdit{
    color: black;
    padding-left: 5px;
    font-size: 16px;
