}

/* ******************QMenu****************** */
QMenu {
    color: white;
    background-color: rgb(85,170,255);
    margin: 2px; 
}

QMenu::item {
    height: 23px;
    padding: 0px 25px 0px 20px;
    background: rgb(85,170,255);
}

QMenu::item:selected {
    background-color: rgb(34, 125, 155);
    border-left: 5px solid rgb(255, 127, 39)
}

QMenu::icon:checked { /* appearance of a 'checked' icon */
    background: rgb(34, 125, 155);
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
 }

QMenu::item:disabled
{
    background: rgb(125, 125, 125);
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
}


QMenu::separator {
    height: 0px;
    background: lightblue;
    margin-left: 10px;
    margin-right: 5px;
}

QMenu::indicator {
    width: 13px;
    height: 13px;
}
/* ******************QMenu****************** */


/* ******************QStatusBar****************** */
QStatusBar {
